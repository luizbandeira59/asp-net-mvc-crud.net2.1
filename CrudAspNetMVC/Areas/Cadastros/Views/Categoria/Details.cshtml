@model Modelo.CadastrosBLL.Categoria

@{
	ViewData["Title"] = "Detalhes";
	Layout = "~/Views/Shared/_LayoutIES.cshtml";
}

<link href="~/assets/css/estilo-botão-tabela.css" rel="stylesheet" />


<div class="content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-9 centro">
				<div class="card">
					<hr />
					<div class="header">
						<h4 class="title">@ViewData["Title"]</h4>
					</div>
					@{await Html.RenderPartialAsync("~/Areas/Cadastros/Views/Categoria/_PartialDetailsContent.cshtml", Model); }

				</div>
			</div>
		</div>
	</div>
</div>



	
