<!DOCTYPE html>

<html>
<head>

	<meta charset="utf-8" />
	<link rel="icon" type="image/png" href="assets/img/favicon.ico">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />a
	<title>Controle Pessoal</title>
	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
	<meta name="viewport" content="width=device-width" />

	<environment include="Development">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css" />
		<link href="~/assets/css/bootstrap1.min.css" rel="stylesheet" />
		<link href="~/assets/css/font-awesome.min.css" rel=" stylesheet" />
		<link href="~/assets/css/animate.min.css" rel="stylesheet" />
		<link href="~/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />
		<link href="~/assets/css/light-bootstrap-1.css" rel="stylesheet" />
	</environment>
	<environment exclude="Development">
		<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet"
			  asp-fallback-href="~/assets/css/font-awesome.min.css" />
	</environment>


	@RenderSection("Styles", false)
</head>
<body>
	<partial name="_CookieConsentPartial" />

	<div class="wrapper">
		<div class="sidebar" data-color="" data-image="../../assets/img/sidebar-4.jpg">
			<div class="sidebar-wrapper">
				<div class="logo">
					<a class="simple-text" asp-controller="Controles" asp-action="Index">
						Controle Pessoal
					</a>
				</div>
				<ul class="nav">
					<li class="active">
						<a asp-controller="Controles" asp-action="Index">
							<i class="pe-7s-graph"></i>
							<p>Painel de Controle</p>
						</a>
					</li>
					<li>
						<a asp-controller="ListaMercados" asp-action="Index">
							<i class="pe-7s-cart"></i>
							<p>Supermercado</p>
						</a>
					</li>
					<li>
						<a asp-controller="DespesaFixas" asp-action="Index">
							<i class="fa fa-usd"></i>
							<p>Despesas Fixas</p>
						</a>
					</li>
					<li>
						<a asp-controller="DespesaDiretas" asp-action="Index">
							<i class="pe-7s-calculator"></i>
							<p>Despesas Diretas</p>
						</a>
					</li>
					<li>
						<a asp-controller="ListaDesejos" asp-action="Index">
							<i class="pe-7s-note2"></i>
							<p>Lista de Desejos</p>
						</a>
					</li>
					<li>
						<a asp-controller="Produto" asp-action="Index">
							<i class="pe-7s-box1"></i>
							<p>Produtos</p>
						</a>
					</li>
					<li>
						<a asp-controller="Controles" asp-action="Index">
							<!--  icone 1<i class="pe-7s-config"></i>  -->
							<!--icone 2--><i class="fa fa-cog fa-spin fa-3x fa-fw"></i>
							<span class="sr-only">Loading...</span>
							<p>Configurações</p>
						</a>
					</li>
					<li class="divider">
					</li>
				</ul>
			</div>
		</div>

		<partial name="_CookieConsentPartial" />

		<div class="main-panel">
			<nav class="navbar navbar-default navbar-fixed">
				<div class="container-fluid">
					<div class="navbar-header">
						<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<a class="navbar-brand" asp-controller="Controle" asp-action="Index"></a>
						<!-- redireciona a algum link, estava dashbord -->
					</div>
					<div class="collapse navbar-collapse">
						<ul class="nav navbar-nav navbar-left">
							<li>
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">

									<i class="fa fa-dashboard"></i>
									<p class="hidden-lg hidden-md">Controle</p>
								</a>
								<ul class="dropdown-menu">
									<li><a href="#">Ação</a></li>
									<!--pode ser uma drop down para mês ou algo do genero-->
								</ul>
							</li>
							<li class="dropdown">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									<i class="fa fa-globe"></i>
									<b class="caret hidden-lg hidden-md"></b>
									<p class="hidden-lg hidden-md">
										5 Notificações
										<b class="caret"></b>
									</p>
								</a>
								<ul class="dropdown-menu">
									<li><a href="#">Notificação 1</a></li>
									<li><a href="#">Notificação 2</a></li>
									<li><a href="#">Notificação 3</a></li>
									<li><a href="#">Notificação 4</a></li>
									<li><a href="#">Mais uma notificação</a></li>
								</ul>
							</li>
							<li>
								<a href="">
									<!-- buscador precisa de uma action -->
									<i class="fa fa-search"></i>
									<p class="hidden-lg hidden-md">Search</p>
								</a>
							</li>
						</ul>

						<ul class="nav navbar-nav navbar-right">
							<li>
								<a href="">
									<p>Conta</p>
								</a>
							</li>
							<li class="dropdown">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									<p>
										Categorias
										<b class="caret"></b>
									</p>
								</a>
								<ul class="dropdown-menu">
									<li><a href="#">Ação link</a></li>
									<li><a href="#">Ação link</a></li>
									<li><a href="#">Ação link</a></li>
									<li><a href="#">Ação link</a></li>
									<li><a href="#">Ação link</a></li>
									<li class="divider"></li>
									<li><a href="#">Ação link</a></li>
									<!--precisa de asp para categorias,ou data, ou algo que va na drop down-->
								</ul>
							</li>
							<li>
								<a href="#">
									<!--asp para metodo encerrar sessão, logout ou algo do tipo-->
									<p>Sair</p>
								</a>
							</li>
							<li class="separator hidden-lg hidden-md"></li>
						</ul>
					</div>
				</div>
			</nav>

			@RenderBody()

			<footer class="footer">
				<div class="container-fluid">
					<nav class="pull-left">
					</nav>
					<p class="copyright pull-right">
						&copy;
						<script>document.write(new Date().getFullYear())</script> <a href="http://www.google.com.br">TCC SENAI</a>
					</p>
				</div>
			</footer>
		</div>
	</div>

	<environment include="Development">
		<script src="~/lib/jquery/dist/jquery.js"></script>
		<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
		<script src="~/js/site.js" asp-append-version="true"></script>
		<script src="~/assets/js/jquery.3.2.1.min.js" type="text/javascript"></script>
		<script src="~/assets/js/bootstrap.min.js" type="text/javascript"></script>
		<script src="~/assets/js/chartist.min.js"></script>
		<script src="~/assets/js/bootstrap-notify.js"></script>
		<script src="~/assets/js/light-bootstrap-dashboard.js"></script>
		<script src="~/assets/js/demo.js"></script>

	</environment>
	<environment exclude="Development">
		<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
				asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
				asp-fallback-test="window.jQuery"
				crossorigin="anonymous"
				integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
		</script>
		<script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
				asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
				asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
				crossorigin="anonymous"
				integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
		</script>
		<script src="~/js/site.min.js" asp-append-version="true"></script>
		
	</environment>

	@RenderSection("Scripts", false)
</body>
</html>

